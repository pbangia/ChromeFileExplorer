{
  "manifest_version": 2,
  "name": "Chrome File Explorer",
  "version": "0.1",
  "permissions": [
    "file://*/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.min.js", "content.js", "config.js", "resources.js","tree.js","jquery.min.js","tree.jquery.js","index.js"]
    }
  ],
  "browser_action": {
     "default_icon": "resources/file_icon.png"
  },
  "background": {
     "scripts": ["background.js"]
  }
}
