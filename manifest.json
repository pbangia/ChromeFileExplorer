{
  "manifest_version": 2,
  "name": "Chrome File Explorer",
  "version": "0.1",
  "permissions": [
    "file://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "file:///*"
      ],
      "js": ["jquery.min.js", "content.js"]
    }
  ],
  "browser_action": {
     "default_icon": "file_icon.png"
  },
  "background": {
     "scripts": ["background.js"]
  }
}
